<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="basisproject.lym.org.basisproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".bottom_bar.BottomBarActivity" />
        <activity android:name=".divider.RecyclerViewDividerActivity" />
        <activity android:name=".dialog.DialogActivity" />
        <activity android:name=".dropdown.DropDownActivity" />
        <activity android:name=".dragimageview.DragImageViewActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>